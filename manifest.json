{
  "manifest_version": 2,

  "name": "Time-waster tracker",
  "description": "Tracks how much time do you waste your sepcified website(s)",
  "version": "1.0",
  "icons": { "128": "icon-128.png" },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Track This site?",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}